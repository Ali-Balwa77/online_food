{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES6",
    "sourceMap": true,
    "lib": ["DOM", "ES6", "ESNext.AsyncIterable"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "moduleResolution": "node",
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
 "exclude": [
   "../node_modules"
 ]
}